    sfl2File="${HOME}/Library/Preferences/com.apple.finder.plist"
elif [ "${currentAppID}" = "com.sublimetext.3" ]; then
    sfl2File="${HOME}/Library/Application Support/Sublime Text 3/Local/Auto Save Session.sublime_session"
else
    sfl2File="${HOME}/Library/Application Support/com.apple.sharedfilelist/com.apple.LSSharedFileList.ApplicationRecentDocuments/${currentAppID}.sfl2"
fi


if [ -f "${sfl2File}" ]; then
    /usr/bin/python main.py "${sfl2File}"
else
    echo '{"items": [{"title": "None Recent Record for the foremost app","subtitle": "(*´･д･)?"}]}'
fi</string>
